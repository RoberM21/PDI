<div class="container z-depth-3 panel">
  <h3 class=" titulo">Usuarios</h3>
  <%= link_to 'Registrar', new_user_path, class:'btn bo' %>
    <table class="striped centered" id="my-table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Email confirmed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.email_confirmed %></td>
        <td><%= link_to 'Eliminar', user, method: :delete, data: { confirm: 'Are you sure?' },class:'btn red' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<script type="text/javascript">
$(document).ready(function(){
    $('#my-table').dynatable();
  });
</script>
